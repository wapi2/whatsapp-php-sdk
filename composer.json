{
    "name": "wapi2/whatsapp-php-sdk",
    "description": "SDK PHP para la API de WhatsApp Web",
    "type": "library",
    "version": "1.1.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wapi2\\WhatsApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wapi2\\WhatsApp\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "keywords": [
        "whatsapp",
        "api",
        "sdk",
        "php",
        "wapi2"
    ],
    "homepage": "https://github.com/wapi2/whatsapp-php-sdk",
    "support": {
        "issues": "https://github.com/wapi2/whatsapp-php-sdk/issues"
    }
}